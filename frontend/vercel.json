{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "devCommand": "npm run dev",
  "env": {
    "VITE_API_URL": "@vite_api_url",
    "VITE_ADMIN_KEY": "@vite_admin_key"
  },
  "routes": [
    {
      "src": "^/(?!.*\\.(?:js|css|svg|png|jpg|jpeg|gif|ico|woff|woff2)$).*$",
      "dest": "/index.html"
    }
  ]
}

